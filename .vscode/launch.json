{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Duploctl Pipe",
      "type": "debugpy",
      "request": "launch",
      "program": "src/pipe.py",
      "console": "integratedTerminal",
      "consoleName": "Duploctl Pipe",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "HOST": "${env:DUPLO_HOST}",
        "TENANT": "${env:DUPLO_TENANT}",
        "TOKEN": "${env:DUPLO_TOKEN}",
        "CONFIG": "${workspaceFolder}/config/duploconfig.yaml",
        "KIND": "version",
        // "CMD": "list",
        // "QUERY": "[].AccountName"
      }
    },
  ]
}
